import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		/*Exemplo para ler variavel Scrig - digitado no teclado apresentar console*/
		System.out.println("=== Dige uma Palavra ========");
		
		Scanner sc = new Scanner(System.in);
		
		String x;
		x = sc.next();
		
		System.out.println("Voce Digitou:" + x );
		
		sc.close();
	}

}
